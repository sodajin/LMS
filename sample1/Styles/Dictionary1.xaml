<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Images for LogInView transition -->
    <BitmapImage x:Key="img01" UriSource="/Resources/LogInBackground/IMG_01.png"/>
    <BitmapImage x:Key="img02" UriSource="/Resources/LogInBackground/IMG_02.png"/>
    <BitmapImage x:Key="img03" UriSource="/Resources/LogInBackground/IMG_03.png"/>
    <BitmapImage x:Key="img04" UriSource="/Resources/LogInBackground/IMG_04.png"/>
    <BitmapImage x:Key="img05" UriSource="/Resources/LogInBackground/IMG_05.png"/>
    <BitmapImage x:Key="img06" UriSource="/Resources/LogInBackground/IMG_06.png"/>
    <BitmapImage x:Key="img07" UriSource="/Resources/LogInBackground/IMG_07.png"/>
    <BitmapImage x:Key="img08" UriSource="/Resources/LogInBackground/IMG_08.png"/>

    <Style x:Key="HeaderTableStyle" TargetType="{x:Type GridViewColumnHeader}">
        <Setter Property="Background" Value="#b6b6b6" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="Padding" Value="0, 5" />
        <Setter Property="BorderBrush" Value="{x:Null}" />
    </Style>
</ResourceDictionary>